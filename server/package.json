{
  "name": "node-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon ./src/index.js --exec babel-node",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel server -s -d dist",
    "production": "npm run build && nodemon bin/production",
    "migration:generate": "cd src && sequelize migration:generate",
    "model:generate": "cd src && sequelize model:generate",
    "migrate": "cd src && sequelize db:migrate",
    "migrate:down": "cd src && sequelize db:migrate:undo"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "koa": "^2.4.1",
    "koa-basic-auth": "^2.0.0",
    "koa-bodyparser": "^4.2.0",
    "koa-cors": "0.0.16",
    "koa-jwt": "^3.3.0",
    "koa-logger": "^3.1.0",
    "koa-response-time-precise": "^2.0.0",
    "koa-router": "^7.3.0",
    "mysql2": "^1.5.1",
    "nodemon": "^1.11.0",
    "sequelize": "^4.23.2",
    "sequelize-cli": "^3.2.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1"
  }
}
